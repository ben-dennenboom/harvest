{
    "name": "dennenboom/harvest",
    "description": "Simple deployment automation tool for Laravel applications",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dennenboom",
            "email": "info@dennenboom.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
